{
  "name": "artboards_to_svg",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "artboards_to_svg",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
